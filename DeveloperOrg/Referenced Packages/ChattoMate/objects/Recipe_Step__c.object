    <validationRules>
        <fullName>Null_date_comparison_check</fullName>
        <active>true</active>
        <description>Date fields can be compared against NULL using &apos;= (Equal To)&apos; or &apos;&lt;&gt; (Not Equal To).</description>
        <errorConditionFormula>ISPICKVAL(Field_Type__c, &apos;date&apos;)
&amp;&amp;
(
  BEGINS(TEXT(Operator__c), &apos;&lt;&apos;) ||
  BEGINS(TEXT(Operator__c), &apos;&lt;=&apos;) || 
  BEGINS(TEXT(Operator__c), &apos;&gt;&apos;) || 
  BEGINS(TEXT(Operator__c), &apos;&gt;=&apos;)
)
&amp;&amp; ISNULL(Date_Value__c)
&amp;&amp; NOT(BEGINS(TEXT(Operator__c), &apos;&lt;&gt;&apos;))</errorConditionFormula>
        <errorMessage>You can only use the &apos;Equal To&apos; and &apos;Not Equal To&apos; operators when comparing date fields with NULL.</errorMessage>
    </validationRules>
